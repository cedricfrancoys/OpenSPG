{% extends '::base.html.twig' %}

{% block title %}Producers{% endblock %}

{% block pageTitleText %}Registration{% endblock %}

{% block body %}

	{{ parent() }}

    <div class="container">
	    {{ form_start(form) }}
			{{ form_errors(form) }}
			{{ form_row(form.Member) }}
			{{ form_row(form.phone) }}
    		{{ form_row(form.Properties) }}
		{{ form_end(form) }}
	</div>

    <!-- FOOTER -->
    <footer>
		<div class="container">
        <p class="pull-right"><a href="#">Back to top</a></p>
        <p><a href="#">{% trans %}Privacy{% endtrans %}</a> &middot; <a href="#">{% trans %}Terms{% endtrans %}</a></p>
		</div>
    </footer>

{% endblock %}

{% block stylesheets %}
	{{ parent() }}
{% endblock %}	
{% block javascripts %}
	{{ parent() }}
	<script src="{{ url('bazinga_jstranslation_js', { 'domain': 'property' }) }}"></script>
	{% javascripts
        'bundles/producer/js/*' %}
        <script src="{{ asset_url }}"></script>
    {% endjavascripts %}
{% endblock %}