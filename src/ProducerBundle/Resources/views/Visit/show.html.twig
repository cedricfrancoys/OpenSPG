{% extends '::base.html.twig' %}

{% block title %}{{"Visit"|trans({},"visit")}}{% endblock %}

{% block pageTitleText %}{{"Visit"|trans({},"visit")}}{% endblock %}

{% block body %}

	{{ parent() }}

    <div class="container add">
		<div class="row">
			{{ include('ProducerBundle:Partials:sidebar.html.twig') }}
			<div class="col-sm-9 col-sm-offset-2 col-md-10 col-md-offset-0 main">
		    	{% for type, messages in app.session.flashbag.all() %}
					{% for message in messages %}
						<div class="alert alert-{{ type }}">{{ message }}</div>
					{% endfor %}
				{% endfor %}
			    {{ include('ProducerBundle:includes:visit_show.html.twig') }}
			</div>
		</div>
	</div>

{% endblock %}

{% block stylesheets %}
	{{ parent() }}
	{% stylesheets 'bundles/producer/css/*' filter='cssrewrite' %}
        <link rel="stylesheet" href="{{ asset_url }}" />
    {% endstylesheets %}
{% endblock %}	
{% block javascripts %}
	{{ parent() }}
{% endblock %}